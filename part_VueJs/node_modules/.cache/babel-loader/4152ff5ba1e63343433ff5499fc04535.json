CAAA","sourcesContent":["<template>\n\n  <table class=\"table\" style=\"width: 80%; margin: 0 auto;\">\n    <thead>\n    <tr>\n      <th>Album</th>\n      <th>Prix</th>\n      <th>Pochette</th>\n      <th>Quantité</th>\n      <th></th>\n    </tr>\n    </thead>\n    <tbody>\n    <tr v-for=\"(a, index) in panier\" :key=\"index\">\n      <td>{{ a.titre }}</td>\n      <td>{{ a.prix / 100 }}€</td>\n      <td><img :src=\"pathImg(a.chemin_image)\" style=\"height: 80px; width: 80px;\"></td>\n      <td> item.quantity</td>\n      <td>\n        <button class=\"btn btn-danger btn-block\" @click=\"destroy(index)\">supprimer<i class=\"fa fa-trash-o fa-lg\"/>\n        </button>\n      </td>\n    </tr>\n    </tbody>\n    <tfoot>\n    <tr>\n      <td colspan=\"3\" class=\"text-right\">Nombre d'articles :</td>\n      <td>{{ nbrElePanier() }}</td>\n      <td></td>\n    </tr>\n    <tr>\n      <td colspan=\"3\" class=\"text-right\">Total :</td>\n      <td>{{ totalPanier() }}€</td>\n      <td></td>\n    </tr>\n    </tfoot>\n  </table>\n</template>\n\n<script>\n\nexport default {\n  name: \"Panier\",\n  computed: {\n    albums() {\n      let albums = new Array();\n      this.$store.state.data.forEach((key) => {\n        if (key.name == \"album\") {\n          albums.push(key.data);\n        }\n      })\n      return albums[0];\n    },\n    panier() {\n      return this.$store.state.panier;\n    },\n    quantite(){\n      return this.$store.state.quantite;\n    }\n  },\n  methods: {\n    destroy(index) {\n      this.panier.splice(index, 1);\n    },\n    pathImg(str) {\n      return \"images/album/\" + str;\n    },\n    totalPanier() {\n      let total = 0;\n      this.$store.state.panier.forEach((key) => {\n        total += key.prix / 100;\n      })\n      return total;\n    },\n    nbrElePanier() {\n      return this.$store.state.panier.length;\n    }\n  }\n}\n\n</script>\n\n<style scoped>\n\n</style>"],"sourceRoot":"src/components"}]}