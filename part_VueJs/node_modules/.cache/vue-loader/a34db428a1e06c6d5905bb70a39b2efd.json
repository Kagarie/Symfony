y>\n        <tfoot>\n        <tr>\n          <td colspan=\"3\" class=\"text-right\">Nombre d'articles :</td>\n          <td>{{ nbrElePanier }}</td>\n          <td></td>\n        </tr>\n        <tr>\n          <td colspan=\"3\" class=\"text-right\">Total :</td>\n          <td>{{ totalPanier }}€</td>\n          <td></td>\n        </tr>\n        </tfoot>\n      </table>\n    </div>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: \"catalogue\",\n  data() {\n    return {\n      input: {ajouter: ''},\n    }\n  },\n  computed: {\n    albums() {\n      return this.$store.getters.getAlbums[0];\n    },\n    panier() {\n      return this.$store.state.panier;\n    },\n    quantite() {\n      return this.$store.state.quantite;\n    },\n    totalPanier() {\n      return this.$store.getters.totalPanier;\n    },\n    nbrElePanier() {\n      return this.$store.getters.nbrElePanier;\n    }\n  },\n  methods: {\n    add(album) {\n      this.$store.commit('add', album)\n    },\n    pathImg(str) {\n      return \"images/album/\" + str;\n    },\n  }\n}\n</script>\n\n<style scoped>\n\n</style>"]}]}